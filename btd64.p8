pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
--game data

map_pts = {{-1,3},{9,3},{9,6},{1,6},{1,8},{11,8},{11,16}}

--speed,img,size,{splits to},{{color from,color to}}
bloon_types = {
	{1, 48, 1, {}, {}},
	{2, 48, 1, {1}, {8,12}},
}

function _init()
	def_monkeys()
	spwn_monkey({9*8+4,2*8+4},1)
end
-->8
--game

round = 0
playing = true

function main()
	mv_crsr()
	mv_bloons()

	map()
	draw_ui()
	rect(crsr[1]*8-1, crsr[2]*8-1, crsr[1]*8+8, crsr[2]*8+8, 7)
	
	draw_bloons()
	update_monkeys()
end

function _update()
	main()
end
-->8
--bloons

bloons = {{1,{-8,24},1}}

function draw_bloons()
	for k,v in pairs(bloons) do
		img = bloon_types[v[1]][2]
		spr(img, v[2][1], v[2][2])
	end
end

function mv_bloons()
	for k,v in pairs(bloons) do
		mv = {0,0}
		pt = map_pts[v[3]]
		spd = bloon_types[v[1]][1]
		next_pt = map_pts[v[3]+1]
		
		if pt[1] < next_pt[1] then
			mv[1] = spd
		elseif pt[1] > next_pt[1] then
			mv[1] = -spd
		end
		if pt[2] < next_pt[2] then
			mv[2] = spd
		elseif pt[2] > next_pt[2] then
			mv[2] = -spd
		end
		
		nx = v[2][1] + mv[1]
		ny = v[2][2] + mv[2]
		
		bloons[k][2] = {nx, ny}
		
		if nx/8 == next_pt[1] and
					ny/8 == next_pt[2] do
		bloons[k][3] += 1
		end
	end
end
-->8
--player

cash = 650
lives = 100
crsr = {0,0}

function mv_crsr()
	if btnp(⬅️) do
		crsr[1] -= 1
	elseif btnp(➡️) do
		crsr[1] += 1
	end
	if btnp(⬆️) do
		crsr[2] -= 1
	elseif btnp(⬇️) do
		crsr[2] += 1
	end
	
	if crsr[1] < 0 do
		crsr[1] = 0
	elseif crsr[1] > 15  do
		crsr[1] = 15
	end
	if crsr[2] < 1 do
		crsr[2] = 1
	elseif crsr[2] > 15 do
		crsr[2] = 15
	end
end
-->8
--ui

-- -1 none
-- 0 build
-- <monkey id> upgrade
in_menu = -1

function rectborder(x1,y1,x2,y2,clr1,clr2)
	rectfill(x1,y1,x2,y2,clr1)
	rect(x1,y1,x2,y2,clr2)
end

function draw_menu()
	w = 48
	rectborder(128-w,0,127,127,4,15)
	if in_menu == 0 then
		
	end
end

function draw_ui()
	draw_topbar()
	if in_menu != -1 then
		draw_menu()
	end
end

function draw_topbar()
	rectfill(0,0,128,8,0)
	
	round_clr = 7
	if playing then
		round_clr = 8
	end
	print("round "..round,0,0,round_clr)
	print("$"..cash,52,0,10)
	print("♥"..lives,108,0,8)
end
-->8
--monkeys data

monkey_types = {}

function def_monkeys()
	mdart = {
		p={0,0},
		i=1,
		r=2,
		a=reg_attack,
		pp=1,
		pr=1,
		l=false,
		cer=false,
		adc=0,
		ad=10,
		u1={
			function (this,i)
				this.pr += 1
			end
		},
		u2={
			function (this,i) 
				this.r += 1
			end
		}
	}
	
	
	monkey_types = {mdart}
end
-->8
--monkeys

--{x,y},{mx,my},pr,lead,cer
proj = {}
monkeys = {}

function update_monkeys()
	for k,v in pairs(monkeys) do
		spr(v.i, v.p[1]-4, v.p[2]-4)
		circ(v.p[1],v.p[2],v.r*8,0)
		v.a(v,k)
		if v.adc > 0 then
			v.adc -= 1
		end
	end
end

function spwn_monkey(pos,ti)
	n = monkey_types[ti]
	n.p = pos
	add(monkeys,n)
end

--sort
--0 first
--1 strong
function bloon_near(pos,r,sort)
	
	for k,v in pairs(bloons) do
		x = v[2][1]
		y = v[2][2]
		tx = pos[1]
		ty = pos[2]
		dx = (x-tx)
		dy = (y-ty)
		d = sqrt(dx^2+dy^2)
		if d < r*8 then
			return v,dx,dy,d
		end
	end
	return 0,0,0,0
end

function reg_attack(this)
	print(#proj)
	b,dx,dy,d =	bloon_near(this.p,this.r,0)
	if b != 0 then
		print(this.adc)
		print(this.ad)
		if this.adc == 0 then
			this.adc = this.ad
			print("shoot")
			x = this.p[1]
			y = this.p[2]
			mx = dx/d
			my = dy/d
			print(mx.." "..my)
			line(x,y,x+mx*this.r*8,y+my*this.r*8,7)
			add(proj,{
				{x,y},
				{mx,my},
				this.pr,
				this.l,
				this.c
			})
		end
	end
end
